server {
    listen 127.0.0.1:1234;

    root /nfs/homes/aharrass/webserv;
    client_max_body_size 5000000000000000000000000000;
    #error_page 404 /nfs/homes/aharrass/webserv/index.html;
    #index index.html;

    location / {
        allow GET;
        allow POST;
        allow DELETE;
        #upload /nfs/homes/aharrass/webserv/www/uploads;
        cgi php /usr/bin/php-cgi;
        cgi py /usr/bin/python3;
        autoindex on;
    }

    location /42 {
        allow GET;
        allow POST;
        allow DELETE;
        cgi php /usr/bin/php-cgi;
        cgi py /usr/bin/python3;
        autoindex on;
        root /nfs/homes/aharrass/webserv/;
        index index.html;
    }

    location  /webserv  {
        root /nfs/homes/aharrass;
        #index index.html;
        autoindex on;
        allow POST;
        allow GET;
        upload /nfs/homes/aharrass/webserv/;
        cgi py /usr/bin/python3;
        cgi php /usr/bin/php-cgi;
    }

    location   /test/barr  {
        root /var/www;
        allow GET;
        index index1.html;
        autoindex on;
    }
}
