    #error_page 404 /nfs/homes/ohalim/Desktop/webserv/index.php;
    #index index.html;

    location / {
        allow GET;
        allow POST;
        allow DELETE;
        #index index.php;
        upload /nfs/homes/ohalim/Desktop/webserv/;
        #cgi php /usr/bin/php-cgi;
        #cgi py /usr/bin/python3;
        autoindex on;
    }

    location  /webserv  {
        root /nfs/homes/ohalim/Desktop;
        #index index.html;
        autoindex on;
        allow POST;
        allow GET;
        allow DELETE;
        #cgi php /usr/bin/php-cgi;
        upload /nfs/homes/ohalim/Desktop/webserv;
        #return 301 https://www.youtube.com;
    }

