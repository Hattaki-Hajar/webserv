server {
    listen 127.0.0.1:1234;

    root /var/www/wordpress;
    index index.php;

    location /var/www {
        root /etc/nginx;
        try_files $uri $uri/ =404;
    }

    location    /var/www/html/  {
        root /etc/logs;
        try_files $uri $uri/ =404;
    }
}
server  {
    listen 127.0.1.1:8080;

    root /var/www/html;
    index index.html;

    location / {
        try_files $uri $uri/ =404;
    }
}