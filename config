server {
    listen 127.0.0.1:1234;

    root /nfs/homes/ohalim/Desktop/webserv/;
    client_max_body_size 5000000000000000000000000000;
    #index index.html;
	server_name shi_haja;

    location / {
        allow GET;
        allow POST;
        allow DELETE;
        upload /nfs/homes/ohalim/Desktop/webserv;
        cgi php /usr/bin/php-cgi;
        cgi py /usr/bin/python3;
        autoindex on;
    }

    location /42 {
        allow GET;
        allow POST;
        allow DELETE;
        cgi php /usr/bin/php-cgi;
        cgi py /usr/bin/python3;
        autoindex on;
        root /nfs/homes/aharrass/webserv/;
        index index.html;
    }

    location  /webserv  {
        root /nfs/homes/ohalim/Desktop;
        #index index.html;
        autoindex on;
        allow POST;
        allow GET;
        allow DELETE;
        upload /nfs/homes/ohalim/Desktop/webserv/;
        cgi py /usr/bin/python3;
        cgi php /usr/bin/php-cgi;
    }

    location   /test/barr  {
        root /var/www;
        allow GET;
        index index1.html;
        autoindex on;
    }
}
