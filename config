server {
    listen 127.0.0.1:1234;

    root /;
    client_max_body_size 5;
    #index index1.html;

    location / {
        allow GET;
        autoindex on;
    }

    location  /webserv  {
        root /nfs/homes/aharrass;
        #index index5.html;
        autoindex on;
        allow POST;
        allow GET;
		upload /nfs/homes/aharrass/webserv;
        cgi py /usr/bin/python3;
        cgi sh /bin/bash;
    }

    location   /test/barr  {
        root /var/www;
        allow GET;
        index index1.html;
        autoindex on;
    }
}
