server {
    listen 127.0.0.1:1234;

    root /var/www/wordpress;
    index index.php;

    location / {
        autoindex on;
    }

    location   /test  {
        #root /var/www/html;
        index index.html;
        autoindex on;
    }

    location   /test/barr  {
        root /var/www;
        index index1.html;
        autoindex on;
    }
}
